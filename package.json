  "author": {
    "name": "Metagraph Team",
    "email": "team@metagraph.io"
  },
  "bugs": {
    "url": "https://github.com/openmg/metagraph-node/issues"
  },
  "homepage": "https://github.com/openmg/metagraph-node",
  "repository": {
    "type": "git",
    "url": "https://github.com/openmg/metagraph-node"
  },
  "keywords": [
    "graph",
    "database",
    "tinkerpop",
    "gremlin",
    "traversals"
  ],
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "bluebird": "3.4.6",
    "es6-enum": "^1.1.0",
    "lodash": "^4.17.2",
    "node-uuid": "^1.4.7",
    "path": "^0.12.7",
    "pouchdb-node": "^6.0.7"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-flowtype-errors": "^1.5.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.36.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "xo && ava"
  },
  "ava": {
    "require": ["babel-register"]
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "esnext": true,
    "space": true,
    "env": [
      "browser",
      "node"
    ],
    "plugins": [
      "flowtype",
      "flowtype-errors",
      "promise"
    ],
    "rules": {
      "arrow-parens": 0,
      "complexity": 0,
      "import/no-extraneous-dependencies": 0,
      "new-cap": 0,
      "no-underscore-dangle": 0,
      "unicorn/filename-case": 0
    }
  }
}
